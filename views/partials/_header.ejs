<body>
  <h1 style="color: rgb(23, 111, 122)"><strong>Tiny App</strong></h1>
  <% if (!user || user === 'undefined'){ %>
    <div class="row">
    <form  action="/register" method="GET">
      &ensp; <button type="submit" style="background-color:rgb(134, 218, 218);color:cornsilk">Register</button>&emsp;&emsp;
    </form>
    <form  action="/login" method="GET">
        <button type="submit" style="background-color:rgb(134, 218, 218);color:cornsilk">Log in</button>
    </form>
  </div>
  <% } else { %>
    <form action="/logout" method="POST">
      Not <%= user.email %> ?
      <button type="submit" style="background-color:rgb(134, 218, 218);color:cornsilk">Logout</button>
    </form>
  <% } %>
</body>