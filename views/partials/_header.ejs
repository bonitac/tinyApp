<body>
  <h1 style="color: rgb(23, 111, 122); font-family: Arial, Helvetica, sans-serif"><strong>Tiny App</strong></h1>
  <% if (!user){ %>
    <form action="/logout" method="POST">
      Not <%= user.email %> ?
      <button type="submit" style="background-color:rgb(134, 218, 218);color:cornsilk">Logout</button>
    </form>
  <% } else { %>
    <form  action="/register" method="GET">
      <button type="submit" style="background-color:rgb(134, 218, 218);color:cornsilk">Register</button>
    </form>
    <form  action="/login" method="GET">
        <button type="submit" style="background-color:rgb(134, 218, 218);color:cornsilk">Log in</button>
    </form>
  <% } %>
</body>